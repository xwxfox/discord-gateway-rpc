{
  "name": "gateway",
  "version": "1.0.0",
  "description": "ElysiaJS gateway for detached Discord RPC",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": true,
  "author": "xwxfox",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --minify && tsc --emitDeclarationOnly",
    "dev": "bun run ./src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "elysia": "^1.1.0",
    "@elysiajs/eden": "^1.1.0",
    "discord-gateway-rpc": "workspace:*",
    "@paws/storage": "workspace:*",
    "@paws/auth": "workspace:*",
    "pg": "^8.11.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/pg": "^8.11.0",
    "typescript": "^5"
  }
}
